<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>netstandard1.4</TargetFrameworks>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <EmbedUntrackedSources>true</EmbedUntrackedSources>
    <AllowedOutputExtensionsInPackageBuildOutputFolder>$(AllowedOutputExtensionsInPackageBuildOutputFolder);.pdb</AllowedOutputExtensionsInPackageBuildOutputFolder>
    <Company />
    <RootNamespace>USAddress</RootNamespace>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <IncludeSource>true</IncludeSource>
    <Authors>Eslam Kamal</Authors>
    <Description>This is a partial port of the Perl Geo::StreetAddress::US CPAN module to C#.</Description>
    <Copyright>Copyright © Eslam Kamal 2019</Copyright>
    <PackageLicenseUrl>https://raw.github.com/eslamkamal1985/usaddress/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/eslamkamal1985/usaddress</PackageProjectUrl>
    <PackageIconUrl>https://raw.github.com/eslamkamal1985/usaddress/master/Icons/icon_36386.png</PackageIconUrl>
    <RepositoryUrl>https://github.com/eslamkamal1985/usaddress</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <NeutralLanguage>en-US</NeutralLanguage>
    <Version>1.1.5</Version>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.0.0-beta-63127-02">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers</IncludeAssets>
    </PackageReference>
  </ItemGroup>
</Project>